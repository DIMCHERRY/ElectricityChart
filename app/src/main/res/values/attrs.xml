<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ArrowTextView">
        <attr name="radius" format="dimension" />
        <attr name="arrowWidth" format="dimension" />
        <attr name="arrowHeight" format="dimension" />
        <attr name="bg" format="color" />
    </declare-styleable>

    <declare-styleable name="DMAutoImageView">
        <attr name="image_src" format="reference"/>
    </declare-styleable>
    <declare-styleable name="SearchEdit">
        <attr name="imageWidth" format="integer" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="textHint" format="reference|string" />
    </declare-styleable>
</resources>